# Cargamos los datos de la Premier League a fecha 16-01-2023 -------------------
premier.league.data <- read.csv("data/premier_league_16012023.csv",
encoding = "UTF-8") %>%
filter(Sem. <= 19,
Marcador != "") %>%
select(Jornada = Sem., Fecha, Local, xG.Local = xG, Marcador,
xG.Visitante = xG.1, Visitante)
premier.teams <-  unique(premier.league.data$Local)
matches.by.team <- tibble()
for(team in premier.teams){
for(i in 1:nrow(premier.league.data)){
if(premier.league.data[i,]$Local == team){
matches.by.team <- rbind(matches.by.team, tibble(Equipo = team,
Jornada = premier.league.data[i,]$Jornada,
Fecha = premier.league.data[i,]$Fecha,
Contrario = premier.league.data[i,]$Visitante,
Goles.Favor = strsplit(premier.league.data[i,]$Marcador, "–")[[1]][[1]],
xG.Favor = premier.league.data[i,]$xG.Local,
Goles.Contra = strsplit(premier.league.data[i,]$Marcador, "–")[[1]][[2]],
xG.Contra = premier.league.data[i,]$xG.Visitante))
}
else if(premier.league.data[i,]$Visitante == team){
matches.by.team <- rbind(matches.by.team, tibble(Equipo = team,
Jornada = premier.league.data[i,]$Jornada,
Fecha = premier.league.data[i,]$Fecha,
Contrario = premier.league.data[i,]$Local,
Goles.Favor = strsplit(premier.league.data[i,]$Marcador, "–")[[1]][[2]],
xG.Favor = premier.league.data[i,]$xG.Visitante,
Goles.Contra = strsplit(premier.league.data[i,]$Marcador, "–")[[1]][[1]],
xG.Contra = premier.league.data[i,]$xG.Local))
}
}
}
matches.by.team <- matches.by.team %>%
arrange(Equipo, Jornada)
View(matches.by.team)
View(matches.by.team)
